FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      H2O;
}
// ************************************************************************* //

dimensions          [0 0 0 0 0 0 0];

internalField       uniform 0.0216667;

boundaryField
{
    walls
    {
        type                zeroGradient;
    }

    frontAndBack
    {
        type                empty;
    }
}

